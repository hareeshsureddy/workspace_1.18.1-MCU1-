/*SPI API's */
/*SPI Peripheral clock control*/
void SPI_PeripClkCntr(SPIx_RegDef_t *pSPIx, uint8_t EnorDI){
	if (EnorDI==ENABLE){
			if (pSPIx==SPI1)

				esle if (pSPIx==SPI2)

					esle if (pSPIx==SPI3)
	}else
	{

	}

}
